{
  "name": "ai-studio-template",
  "version": "0.1.0",
  "license": "MIT",
  "description": "The open-source IDE for AI agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/akv004/ai-studio-template.git"
  },
  "type": "module",
  "workspaces": [
    "apps/ui",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/ui",
    "build": "npm run build --workspace=apps/ui",
    "tauri": "cd apps/desktop/src-tauri && cargo tauri",
    "tauri:dev": "cd apps/desktop/src-tauri && cargo tauri dev",
    "tauri:build": "cd apps/desktop/src-tauri && cargo tauri build",
    "sidecar": "python apps/sidecar/server.py"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2"
  }
}